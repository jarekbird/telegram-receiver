{
  "name": "telegram-receiver",
  "version": "1.0.0",
  "description": "Telegram webhook receiver that forwards messages to Cursor Runner API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "lint": "echo 'Linting not yet configured' && exit 0",
    "lint:fix": "echo 'Linting not yet configured' && exit 0",
    "format": "echo 'Formatting not yet configured' && exit 0",
    "format:check": "echo 'Formatting not yet configured' && exit 0",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:unit": "npx jest --testPathPatterns=tests/unit",
    "test:integration": "npx jest --testPathPatterns=tests/integration"
  },
  "keywords": [
    "telegram",
    "webhook",
    "bot",
    "cursor-runner",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/sinon": "^21.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.2",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "ioredis": "^5.8.2",
    "pino": "^10.1.0",
    "redis": "^4.6.10"
  }
}
