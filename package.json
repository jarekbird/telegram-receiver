{
  "name": "telegram-receiver",
  "version": "1.0.0",
  "description": "Telegram webhook receiver that forwards messages to Cursor Runner API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "lint": "echo 'Linting not configured' && exit 0",
    "format:check": "echo 'Formatting not configured' && exit 0"
  },
  "keywords": [
    "telegram",
    "webhook",
    "bot",
    "cursor-runner",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}
